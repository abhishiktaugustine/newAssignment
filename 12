 #include<iostream>
#include<string>

using namespace std;
void zero_Small(int &, int &);

void main() {
	int val1,val2;
	cout << "enter the first number" << endl;
	cin >> val1;
	cout << "enter the second number" << endl;
	cin >> val2;
	zero_Small(val1, val2);
	cout << "Value 1:" << val1<<endl;
	cout << "Value 2:" << val2<<endl;
}
void zero_Small(int &val1,int &val2) {
	if (val1 > val2) {
		val2 = 0;
	}
	else
		val1 = 0;
}
